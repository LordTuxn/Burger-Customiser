<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:startPage="clr-namespace:Burger_Customiser.Navigation.Pages.Start"
                    xmlns:orderOptionPage="clr-namespace:Burger_Customiser.Navigation.Pages.OrderOption"
                    xmlns:articleOptionPage="clr-namespace:Burger_Customiser.Navigation.Pages.ArticleOption">

    <DataTemplate DataType="{x:Type startPage:StartPageVM}">
        <startPage:StartPage />
    </DataTemplate>

    <DataTemplate DataType="{x:Type orderOptionPage:OrderOptionPageVM}">
        <orderOptionPage:OrderOptionPage />
    </DataTemplate>

    <DataTemplate DataType="{x:Type articleOptionPage:ArticleOptionPageVM}">
        <articleOptionPage:ArticleOptionPage />
    </DataTemplate>
</ResourceDictionary>